{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./*"
      ]
    },
    "typeRoots": [
      "./node_modules/@types",
      "./app/types"
    ],
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": false
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "expo-env.d.ts",
    "app/types/**/*.d.ts",
    ".expo/types/**/*.ts"
  ]
}
