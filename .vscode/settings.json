{
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit",
    "source.organizeImports": "explicit",
    "source.sortMembers": "explicit"
  },
  "java.compile.nullAnalysis.mode": "automatic",
  "java.project.resourceFilters": [
    "node_modules",
    "**/node_modules/**",
    "**/expo-modules-autolinking/**",
    "**/expo-modules-core/expo-module-gradle-plugin/**"
  ],
  "java.import.gradle.enabled": true,
  "java.import.gradle.wrapper.enabled": true,
  "java.configuration.updateBuildConfiguration": "automatic",
  "java.errors.incompleteClasspath.severity": "ignore",
  "java.import.gradle.offline.enabled": false,
  "java.import.gradle.java.home": null,
  "java.import.gradle.user.home": null,
  "java.import.gradle.arguments": null,
  "java.import.gradle.jvmArguments": null,
  "java.import.gradle.version": null,
  "files.watcherExclude": {
    "**/node_modules/expo-modules-autolinking/android/expo-gradle-plugin/**": true,
    "**/node_modules/expo-modules-core/expo-module-gradle-plugin/**": true
  },
  "java.project.sourcePaths": [
    "android/app/src/main/java",
    "android/app/src/main/kotlin"
  ],
  "java.project.outputPath": "android/app/build",
  "java.project.referencedLibraries": [
    "android/**/*.jar",
    "android/**/*.aar"
  ]
}
